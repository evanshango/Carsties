{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ClientApp": "http://localhost:3000",
  "ReverseProxy": {
    "Routes": {
      "auctionsRead": {
        "ClusterId": "auctions",
        "Match": {
          "Path": "/auctions/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/v1/auctions/{**catch-all}"
          }
        ]
      },
      "auctionsWrite": {
        "ClusterId": "auctions",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/auctions/{**catch-all}",
          "Methods": [
            "POST",
            "PUT",
            "DELETE"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/v1/auctions/{**catch-all}"
          }
        ]
      },
      "search": {
        "ClusterId": "search",
        "Match": {
          "Path": "/search/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/v1/search/{**catch-all}"
          }
        ]
      },
      "biddingWrite": {
        "ClusterId": "bidding",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/bids",
          "Methods": [
            "POST"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/v1/bids"
          }
        ]
      },
      "biddingRead": {
        "ClusterId": "bidding",
        "Match": {
          "Path": "/bids/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/v1/bids/{**catch-all}"
          }
        ]
      },
      "notifications": {
        "ClusterId": "notifications",
        "CorsPolicy": "carstiesPolicy",
        "Match": {
          "Path": "/notifications/{**catch-all}"
        }
      }
    }
  }
}
